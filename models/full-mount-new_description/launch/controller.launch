<launch>

<rosparam file="$(find full-mount-new_description)/launch/controller.yaml" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="full-mount-new" args="Revolute 1_position_controller Slider 2_position_controller Revolute 3_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/full-mount-new/joint_states"/>
</node>

</launch>